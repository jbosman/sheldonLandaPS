// http://eslint.org/docs/rules/
{
  "root": true,
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": false,
    "browser": false,
    "jquery": false,
    "mocha": false,
    "jasmine": false
  },
  "rules": {
    "global-require": 0, // 0 = off, 1 = warn (yellow), 2 = error (red)
    "camelcase": 1,
    "no-debugger": 1,
    "no-console": 0
    // add more rules here to override the fullstack config
  }
}
